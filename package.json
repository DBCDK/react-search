{
  "name": "react-search",
  "version": "0.0.1",
  "description": "Prototype of a Opensearch search widget implemented with react",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start:hot": "webpack-dev-server --hot --progress --colors",
    "start:watch": "webpack --progress --colors --watch -d",
    "css:watch": "node-sass -w -o public --output-style compressed --source-map public/main.css.map  style/main.scss main.css",
    "css:build": "node-sass -o public --output-style compressed style/main.scss main.css",
    "serve": "supervisor -w client,views,lib,routes,server index.js",
    "build": "webpack --progress --colors --optimize-minimize",
    "dev": "parallelshell \"npm run serve -s\" \"npm run css:build\" \"npm run start:watch -s\" \"npm run css:watch -s\""
  },
  "keywords": [
    "opensearch",
    "react",
    "reactive",
    "rxjs",
    "consolidation"
  ],
  "author": "DBC",
  "repository": {
    "type": "git",
    "url": "https://github.com/DBCDK/react-search.git"
  },
  "dependencies": {
    "babel": "^4.6.6",
    "body-parser": "^1.12.0",
    "cache-manager": "^0.18.0",
    "cookie-parser": "^1.3.4",
    "es6-promise": "^2.0.1",
    "express": "^4.12.1",
    "express-handlebars": "^1.2.1",
    "jsdom": "^3.1.2",
    "jsonpath-object-transform": "https://github.com/vibjerg/jsonpath-object-transform/archive/1.0.5.tar.gz",
    "lodash": "^3.3.1",
    "morgan": "^1.5.1",
    "react": "^0.12.2",
    "react-loader": "^1.2.0",
    "react-router": "^0.12.4",
    "react-tools": "^0.12.2",
    "reflux": "^0.2.5",
    "soap": "^0.8.0",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "superagent": "^0.21.0",
    "superagent-promise": "^0.2.0"
  },
  "devDependencies": {
    "babel-core": "^4.6.3",
    "babel-jest": "^4.0.0",
    "babel-loader": "^4.0.0",
    "jasmine-reporters": "^2.0.4",
    "jest-cli": "^0.4.0",
    "jsx-loader": "^0.12.2",
    "node-sass": "^2.0.1",
    "parallelshell": "^1.0.4",
    "react-hot-loader": "^1.1.5",
    "sass-loader": "^0.4.1",
    "watch": "^0.14.0",
    "webpack": "^1.6.0",
    "webpack-dev-server": "^1.7.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ]
  }
}
