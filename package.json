{
  "name": "react-search",
  "version": "0.0.1",
  "description": "Prototype of a Opensearch search widget implemented with react",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start:hot": "webpack-dev-server --hot --progress --colors",
    "start:watch": "webpack --progress --colors --watch -d",
    "serve": "supervisor -w client,views,lib,routes index.js",
    "build": "webpack --progress --colors",
    "dev": "parallelshell \"npm run serve -s\" \"npm run start:watch -s\""
  },
  "keywords": [
    "opensearch",
    "react",
    "reactive",
    "rxjs",
    "consolidation"
  ],
  "author": "DBC",
  "repository": {
    "type": "git",
    "url": "https://github.com/DBCDK/react-search.git"
  },
  "dependencies": {
    "express": "^4.12.1",
    "express-handlebars": "^1.2.1",
    "react": "^0.12.2",
    "react-router": "^0.12.4",
    "react-tools": "^0.12.2",
    "reflux": "^0.2.5",
    "superagent": "^0.21.0",
    "superagent-promise": "^0.2.0"
  },
  "devDependencies": {
    "babel-core": "^4.6.3",
    "babel-loader": "^4.0.0",
    "jasmine-reporters": "^2.0.4",
    "jest-cli": "^0.4.0",
    "jsx-loader": "^0.12.2",
    "parallelshell": "^1.0.4",
    "react-hot-loader": "^1.1.5",
    "sass-loader": "^0.4.1",
    "watch": "^0.14.0",
    "webpack": "^1.6.0",
    "webpack-dev-server": "^1.7.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "react"
    ]
  }
}
